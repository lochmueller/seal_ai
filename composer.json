{
  "name": "lochmueller/seal-ai",
  "type": "typo3-cms-extension",
  "description": "SEAL Search AI - Vector based search based on EXT:seal",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "typo3",
    "cmsig",
    "seal",
    "ai",
    "seal-adapter",
    "search-client",
    "search"
  ],
  "autoload": {
    "psr-4": {
      "Lochmueller\\SealAi\\": "Classes/"
    }
  },
  "require": {
    "php": "^8.3",
    "typo3/cms-core": "^13.4",
    "lochmueller/seal": "0.0.2",
    "symfony/ai-platform": "dev-main#ddb15e9eab8213f8e66f2d3e42952d6f1105b5d8 as 0.0.1",
    "symfony/ai-store": "dev-main#557c02a1cc8c6ba5b06cf0b35232028e3ada8f0b as 0.0.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.84"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "seal_ai",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  },
  "scripts": {
    "code-fix": "PHP_CS_FIXER_IGNORE_ENV=1 .Build/bin/php-cs-fixer fix Classes --rules=@PER-CS2.0:risky,@PER-CS2.0,declare_strict_types,no_unused_imports --allow-risky yes"
  }
}
